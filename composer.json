{
    "name": "blackboxcode/pando-ecomm-bundle",
    "description": "Pando ECommBundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Black Box Code",
            "email": "contact@blackboxcode.com"
        }
